{
    "useTabs": false,
    "tabWidth": 4,
    "arrowParens": "avoid",
    "printWidth": 144,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "htmlWhitespaceSensitivity": "ignore",
    "svelteIndentScriptAndStyle": true,
    "quoteProps": "consistent",
    "plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
